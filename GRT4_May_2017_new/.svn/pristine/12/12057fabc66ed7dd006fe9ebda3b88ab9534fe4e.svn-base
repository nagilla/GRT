package com.grt.dto;

import java.util.List;

import com.avaya.grt.mappers.SiteRegistration;



public class PaginationForSiteRegistration {
	private int maxSize;

	private List<SiteRegistration> list;
	
	private List<RegistrationSummary> summaryList;
	
	public List<SiteRegistration> getList() {
		return list;
	}

	public void setList(List<SiteRegistration> list) {
		this.list = list;
	}

	public int getMaxSize() {
		return maxSize;
	}

	public void setMaxSize(int maxSize) {
		this.maxSize = maxSize;
	}

	public List<RegistrationSummary> getSummaryList() {
		return summaryList;
	}

	public void setSummaryList(List<RegistrationSummary> summaryList) {
		this.summaryList = summaryList;
	}

}
