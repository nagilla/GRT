.autoComplete-list.is-hidden {
	display:none;
}
.autoComplete-list ul,.predictive ul {
	/*position: absolute;*/
	top: 21px;
	right: 27px;
	width: 160px;
	background: #fff;
	overflow-y: scroll;
	box-shadow: 0 2px 5px rgba(0, 0, 0, .5);
	padding: 10px;
	border-bottom-left-radius: 2px;
	border-bottom-right-radius: 2px;
	z-index: 999
	list-style-type: none;
	margin: 0
}

.autoComplete-list ul li,.predictive ul li {
	text-align: left;
	float: none
}

.autoComplete-list ul li a,.predictive ul li a {
	color: #555 !important;
	font-weight: 400;
	text-shadow: none !important;
	border-right: 0;
	transition: .25s ease-out;
	-webkit-transition: .25s ease-out;
	-moz-transition: .25s ease-out;
	line-height: 1.15 !important;
	padding: 5px 0 !important;
	display: block
}

.autoComplete-list ul li a:hover,.predictive ul li a:hover {
	color: #c00 !important;
	text-decoration: underline
}

.autoComplete-list ul li.active a,.predictive ul li.active a {
	background: #eeedea
}
