<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
    name="com.avaya.grt.mappers.CountryAlarmReceiver" 
    table="COUNTRY_ALARM_RECEIVER"
    lazy="false"
>
    <id
        name="countryCode"
        type="string"
        column="COUNTRY_CODE"
        length="50"
    >
        <generator class="assigned" />
    </id>
    <property
        name="phoneNo"
        type="string"
        column="PHONE_NO"
        length="50"
    />

    <!-- Associations -->

</class>
</hibernate-mapping>
