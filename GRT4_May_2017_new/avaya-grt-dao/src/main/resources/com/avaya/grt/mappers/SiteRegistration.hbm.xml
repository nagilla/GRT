    <property name="activeSR" type="string"
		formula="(CONCAT (
             CONCAT (
                (SELECT ibSR.siebel_sr_no
                   FROM sr_request ibSR
                  WHERE ibSR.sr_request_id = INSTALL_BASE_SR_REQUEST_ID),
                (SELECT eqrSR.siebel_sr_no
                   FROM sr_request eqrSR
                  WHERE eqrSR.sr_request_id =
                           FINAL_VALIDATION_SR_REQUEST_ID)),
             (SELECT eqmSR.siebel_sr_no
                FROM sr_request eqmSR
               WHERE eqmSR.sr_request_id = EQR_MOVE_SR_REQUEST_ID)))"/>
    
	<!-- Associations -->
	<!-- bi-directional many-to-one association to ProcessStep -->
	<many-to-one
        name="processStep"
        class="com.avaya.grt.mappers.ProcessStep"
        lazy="proxy"
    >
		<column name="PROCESS_STEP_ID" not-null="true" length="50"/>
	</many-to-one>
	<!-- bi-directional many-to-one association to Status -->
	<many-to-one
        name="status"
        class="com.avaya.grt.mappers.Status"
        lazy="proxy"
    >
		<column name="STATUS_ID" not-null="true" length="50"/>
	</many-to-one>
	<!-- bi-directional many-to-one association to Status -->
	  <many-to-one
        name="installBaseStatus"
        class="com.avaya.grt.mappers.Status"
        lazy="proxy"
    >
		<column name="INSTALL_BASE_STATUSID" not-null="false" length="50"/>
	</many-to-one> 
	<!-- bi-directional many-to-one association to Status -->
	  <many-to-one
        name="techRegStatus"
        class="com.avaya.grt.mappers.Status"
        lazy="proxy"
    >
