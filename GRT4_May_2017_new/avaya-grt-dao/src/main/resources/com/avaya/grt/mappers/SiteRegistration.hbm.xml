	</many-to-one>
	
	<many-to-one
        name="installBaseSubStatus"
        class="com.avaya.grt.mappers.Status"
        lazy="proxy"
    >
		<column name="INSTALL_BASE_SUB_STATUSID" not-null="false" length="50"/>
	</many-to-one> 
	
	<many-to-one
        name="eqrMoveSubStatus"
        class="com.avaya.grt.mappers.Status"
        lazy="proxy"
    >
		<column name="EQR_MOVE_SUB_STATUSID" not-null="false" length="50"/>
	</many-to-one> 
	<!-- bi-directional many-to-one association to TechnicalOrder -->
	<set
        name="technicalOrders"
        inverse="true"
        lazy="true"
        cascade="all"
    >
		<key>
			<column name="SITE_REGISTRATION_ID" />
			 <!-- a foreign key in TECHNICAL_ORDER referencing the primary key of this table. -->
		</key>
		<one-to-many class="com.avaya.grt.mappers.TechnicalOrder" />
	</set>
	<!-- bi-directional many-to-one association to Registration Questions -->
	<set
        name="registrationQuestions"
        inverse="true"
        lazy="true"
        cascade="all"
    >
		<key>
			<column name="SITE_REGISTRATION_ID" />
			 <!-- a foreign key in TECHNICAL_ORDER referencing the primary key of this table. -->
		</key>
		<one-to-many class="com.avaya.grt.mappers.RegistrationQuestions" />
	</set>
	
	<!-- bi-directional one-to-one association to SRRequest -->
    <many-to-one
        name="installBaseSrRequest"
        class="com.avaya.grt.mappers.SRRequest"
        lazy="proxy" unique="true"
    >
		<column name="INSTALL_BASE_SR_REQUEST_ID" length="50"/>
	</many-to-one>
	
	<!-- bi-directional one-to-one association to SRRequest -->
    <many-to-one
        name="finalValidationSrRequest"
        class="com.avaya.grt.mappers.SRRequest"
        lazy="proxy" unique="true"
    >
		<column name="FINAL_VALIDATION_SR_REQUEST_ID" length="50"/>
	</many-to-one>
	
	<!-- bi-directional one-to-one association to SRRequest -->
