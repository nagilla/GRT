        name="eqrActiveContractsSrRequest"
        class="com.avaya.grt.mappers.SRRequest"
        lazy="proxy" unique="true"
    >
		<column name="ACTIVE_CONTRACT_SR" length="50"/>
	</many-to-one>
	
	<!-- bi-directional many-to-one association to RegistrationType -->
    <many-to-one
        name="registrationType"
        class="com.avaya.grt.mappers.RegistrationType"
        lazy="proxy" unique="true"
    >
		<column name="REGISTRATIONTYPEID" length="50"/>
	</many-to-one>

<!-- bi-directional many-to-one association to Registration Questions -->
	<set
        name="salMigratedAssets"
        inverse="true"
        lazy="true"
        cascade="all"
    >
		<key>
			<column name="SITE_REGISTRATION_ID" />
		</key>
		<one-to-many class="com.avaya.grt.mappers.SiteList" />
	</set>
</class>
</hibernate-mapping>
